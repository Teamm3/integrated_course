
.beep-buttom{
	width: 60%;
	text-align: center;
	height: 100rpx;
	margin-top: 10rpx;
	margin-left: 20%;
}
.buttom{
	background: #FFFFFF;
	border-color: #123456;
	border-width: 2rpx;
	border-radius: 15rpx;
	width: 50%;
	font-size: 27rpx;
	color: #123456;
	font-weight: 500;
}
.warning{
	width: 80%;
	text-align: center;
	margin-top: 3rpx;
}
.warning-text{
	color: #DD524D;
	font-size: 30rpx;
	font-weight: bold;
}
.range-item-text{
	display: inline-block;
	margin-top: 26rpx;
	margin-right: 10rpx;
	vertical-align: top;
}
.range-item-input{
	display: inline-block;
	margin-top: 6rpx;
	margin-left: 10rpx;
	margin-right: 10rpx;
}
.light {
	width: 100%;
	height: 40%;
	margin-top: 5%;
}
.beep {
	width: 100%;
	height: 50%;
	margin-top: 3%;
}
.title {
	width: 40%;
	height: 50rpx;
	margin-left: 60rpx;
}
.title-text {
	color: #123456;
	font-size: 36rpx;
	font-weight: bold;
}
.line {
	background: #CC99CC;
	height: 2rpx;
	width: 70%;
	margin-left: 15%;
	margin-top: 10rpx;
}
.light-content {
	/* background: #007AFF; */
	height: 400rpx;
	width: 90%;
	margin-left: 7%;
	margin-top: 10rpx;
}
.beep-content {
	/* background: #007AFF; */
	height: 450rpx;
	width: 90%;
	margin-left: 5%;
	margin-top: 10rpx;
}
.switch {
	-webkit-transform: scale(0.8);
	    -ms-transform: scale(0.8);
	        transform: scale(0.8);
	margin-left: 10rpx;
	color: #123456;
}
.picture {
	display: inline-block;
	width: 40%;
	height: 400rpx;
	margin-top: 15rpx;
	/* padding-bottom: 15upx; */
}
.light-image {
	width: 100%;
	height: 75%;
	padding-bottom: 5%;
}
.switch-text {
	font-size: 30rpx;
	font-weight: 500;
	color: #123456;
}
.switch-item {
	margin-top: 30rpx;
	height: 60rpx;
	width: 80%;
}
.switchs {
	display: inline-block;
	margin-left: 10%;
	width: 50%;
	height: 400rpx;
	vertical-align: top;
	padding-bottom: 250rpx;
}
.auto-beep{
	/* background: #8A6DE9; */
	width: 100%;
	text-align: center;
	vertical-align: middle;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	padding-top: 10rpx;
	height: 80rpx;
}
.beep-range{
	/* background: #333333; */
	width: 100%;
	text-align: center;
	vertical-align: middle;
	height: 200rpx;
	margin-top: 3rpx;
	margin-left: 48rpx;
}
.warning{
	/* background: #B2B2B2; */
	width: 100%;
	height: 50rpx;
	text-align: center;
	vertical-align: middle;
}
.beep-bottom{
	/* background: #123456; */
	width: 100%;
	height: 100rpx;
	text-align: center;
	vertical-align: middle;
}
.beep-range-item{
	width: 80%;
	/* padding-left: 20%; */
	height: 40%;
	padding-top: 2%;
	text-align: center;
	vertical-align: top;
}
.input{
	width: 100rpx;
	height: 50rpx;
	display: inline-block;
	border-color: #CC99CC;
	border-radius: 15rpx;
	border-style: solid;
	border-width: 2rpx;
	color: #123456;
	font-weight: 500;
	font-size: 32rpx;
}
.short-line{
	width: 50rpx;
	height: 2rpx;
	background: #123456;
	margin-top: 20rpx;
}
.line-view{
	width: 50rpx;
	height: 50rpx;
	margin-top: 30rpx;
	/* vertical-align: top; */
	vertical-align: center;
	display: inline-block;
	margin-left: 10rpx;
	margin-right: 10rpx;
}

